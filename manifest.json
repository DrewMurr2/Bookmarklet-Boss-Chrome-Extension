{
    "manifest_version": 2,
    "name": "Bookmarklet Boss",
    "version": "0.1",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "storage"
    ],
    "browser_action": {
        "default_icon": "icon128.png",
        "default_popup": "index.html",
        "default_title": "Run External JS!"
    }
}